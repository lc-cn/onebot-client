  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lc-cn/onebot-client/issues"
  },
  "homepage": "https://github.com/lc-cn/onebot-client#readme",
  "devDependencies": {
    "@types/node": "^18.7.19",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "event-deliver": "^1.0.2"
  }
}
