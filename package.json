{
  "name": "onebot-client",
  "version": "1.0.1",
  "description": "onebot client SDK",
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "tsc",
    "pub": "npm publish --access public"
  },
  "workspaces": [
    "example"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lc-cn/onebot-client.git"
